{
    "env": {
        "inclpath": [
            "${workspaceFolder}/lib/SFML/include",
            "${workspaceFolder}/src"
        ],
        "compiler": "C:/mingw64/bin/g++.exe"
    },
    "configurations": [
        {
            "name": "Game (Windows)",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${inclpath}"
            ],
            "defines": [
                "GAME"
            ],
            "compilerPath": "${compiler}",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}